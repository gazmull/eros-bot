{
  "name": "eros-bot",
  "version": "3.4.0",
  "description": "A simple bot for pulling character information and harem episodes from Kamihime PROJECT Fandom and Kamihime Database.",
  "main": "build/index.js",
  "typings": "typings",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gazmull/eros-bot.git"
  },
  "author": "gazmull <vescalaw@gmail.com> (thegzm.space)",
  "license": "MIT",
  "homepage": "https://github.com/gazmull/eros-bot#readme",
  "bugs": {
    "url": "https://github.com/gazmull/eros-bot/issues"
  },
  "keywords": [
    "kamihime",
    "project",
    "kamihime",
    "discord",
    "bot",
    "discord.js",
    "akairo",
    "kamihime",
    "database"
  ],
  "scripts": {
    "test": "yarn run lint && yarn run compile",
    "compile": "tsc -p .",
    "lint": "tslint -c .tslint.js -p .",
    "lint-fix": "yarn run lint --fix",
    "pm2": "pm2 start .pm2.yml --env production",
    "dev:watch": "tsc -p . --watch",
    "dev:start": "nodemon . --config .nodemon.json",
    "docs:parse": "node . --parseDocs"
  },
  "dependencies": {
    "bufferutil": "^4.0.1",
    "chalk": "^2.4.2",
    "discord-akairo": "^8.0.0-beta.8",
    "discord-paginationembed": "^1.0.0-beta.3",
    "discord.js": "github:discordjs/discord.js",
    "erlpack": "github:discordapp/erlpack",
    "fs-extra": "^7.0.1",
    "infobox-parser": "^3.2.0",
    "mariadb": "^2.0.3",
    "moment-timezone": "^0.5.23",
    "node-fetch": "^2.3.0",
    "nodemw": "^0.12.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "5.3.5",
    "sequelize-typescript": "^1.0.0-alpha.9",
    "twitter-lite": "^0.9.4",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/json2md": "^1.5.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^11.13.4",
    "@types/node-fetch": "^2.3.2",
    "json2md": "^1.6.3",
    "tslint": "^5.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.3"
  }
}

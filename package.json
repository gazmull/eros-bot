{
  "name": "eros-bot",
  "version": "4.1.8",
  "description": "A simple bot for pulling character information and harem episodes from Kamihime PROJECT Fandom and Kamihime Database.",
  "main": "build/index.js",
  "typings": "typings",
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gazmull/eros-bot.git"
  },
  "author": "gazmull <vescalaw@gmail.com> (thegzm.space)",
  "license": "MIT",
  "homepage": "https://github.com/gazmull/eros-bot#readme",
  "bugs": {
    "url": "https://github.com/gazmull/eros-bot/issues"
  },
  "keywords": [
    "kamihime",
    "project",
    "kamihime",
    "discord",
    "bot",
    "discord.js",
    "akairo",
    "kamihime",
    "database"
  ],
  "scripts": {
    "test": "yarn run lint && yarn run compile",
    "compile": "npx tsc -p .",
    "lint": "npx tslint -c .tslint.js -p .",
    "lint-fix": "yarn run lint --fix",
    "pm2": "pm2 start .pm2.yml --env production",
    "dev:watch": "npx tsc -p . --watch",
    "dev:start": "npx nodemon . --config .nodemon.json",
    "docs:build": "node . --parseDocs"
  },
  "dependencies": {
    "@gazmull/logger": "github:gazmull/logger",
    "bufferutil": "^4.0.1",
    "discord-akairo": "^8.0.0-beta.8",
    "discord-paginationembed": "^2.0.0-beta.1",
    "discord.js": "discordjs/discord.js#38d370f",
    "fs-extra": "^8.1.0",
    "infobox-parser": "^3.2.0",
    "mariadb": "^2.1.2",
    "moment-timezone": "^0.5.27",
    "node-fetch": "^2.6.0",
    "nodemw": "^0.12.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0",
    "twitter-lite": "^0.9.4",
    "zlib-sync": "^0.1.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/fs-extra": "^8.0.1",
    "@types/json2md": "^1.5.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.12.7",
    "@types/node-fetch": "^2.5.3",
    "@types/validator": "^10.11.3",
    "json2md": "^1.6.5",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.2"
  }
}
